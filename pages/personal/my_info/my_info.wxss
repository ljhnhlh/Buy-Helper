.top-block{
  display:flex;
  justify-content: center;
  background-color: aqua;
  border-radius: 2%;
  margin: 2px;
}
.s-image{
  border-radius: 50%;
  width:120px;
  height: 120px;
}

.mid-block{
  margin: 2px;
}

.divLine{
 background: rgb(189, 186, 186);
 width: 100%;
 height: 5rpx;
}

.list-item{
  display: flex;
  flex-direction: row;
  margin-top: 10px;
  margin-left: 10px;
  margin-right: 10px;
  margin-bottom: 0px;
  align-items: center;
}

.s-icon{
  width: 40px;
  height: 40px;
  margin-right:20px 
}

.bottom-block{
  margin-top: 20px;
  margin-bottom: 20px;
  display: flex;
  flex-direction: column;
  align-items: center;
}


.size-card{
  border: solid 2px rgb(218, 215, 215);
  width: 120px;
  height: 100px;
  border-radius: 10%;
}

.ident-info{
  width: 100px;
  background-color: red;
}